# Application Settings
NODE_ENV=development
VITE_PORT=3001


# Google OAuth2 Configuration (Required for authentication)
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# URLs and Endpoints
# For local development, use localhost URLs
# For production, set to your deployed domain (e.g., https://api.quozen.com)
VITE_API_URL=http://localhost:3001

# Production Settings (update these values in Vercel environment variables)
# NODE_ENV=production
# VITE_API_URL=https://api.quozen.com

# Access Google Picker
VITE_GOOGLE_PICKER_API_KEY=your-google-picker-api-key

